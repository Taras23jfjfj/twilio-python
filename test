        self.start_time = None
        self.program_start_time = datetime.now()

        self.category_label = tk.Label(master, text="", font=("Arial", 18, "bold"))

        self.start_button = tk.Button(master, text="Почати", command=self.show_categories)
        self.start_button.pack(pady=20)

        self.category_frame = tk.Frame(master)
        self.sound_button = tk.Button(master, text="🔊", command=self.play_word)
        self.word_label = tk.Label(master, text="", font=("Arial", 24))
        self.entry = tk.Entry(master, font=("Arial", 18))
        self.check_button = tk.Button(master, text="Перевірити", command=self.check_answer)
        self.result_label = tk.Label(master, text="", font=("Arial", 18))
        self.button_frame = tk.Frame(master)
        self.skip_button = tk.Button(self.button_frame, text="Пропустити", command=self.skip_word)
        self.next_button = tk.Button(self.button_frame, text="Next", command=self.next_word)
        self.retry_button = tk.Button(self.button_frame, text="Повторити спробу", command=self.retry)
        self.translate_button = tk.Button(self.button_frame, text="Перекласти", command=self.translate)
        
        self.end_button = tk.Button(master, text="Завершити", command=self.end_game, bg='red', fg='white')
        self.end_button.pack(side='top', anchor='ne', padx=10, pady=10)

        
        
        
        self.back_button = tk.Button(master, text="Back", command=self.go_back)
        self.back_button.pack(side='top', anchor='ne', padx=10, pady=10)
        self.back_button.pack_forget()  # Спочатку приховуємо кнопку

        self.entry.bind('<Return>', self.handle_enter)
        self.master.bind('<Shift_R>', self.skip_word)  # Додаємо прив'язку Right Shift
    
        self.translate_pressed = False  # Додаємо новий атрибут
        self.game_started = False
    
    def go_back(self):
        if self.current_category:
            self.pause_category_time()
            self.current_category = None
            self.word_label.config(text="")
            self.show_categories()
        elif self.direction:
            self.direction = None
            self.show_categories()
        else:
            pass
        self.reset_ui()


                
    def show_categories(self):
        self.reset_ui()
        
        if self.direction is None:
            self.direction_frame.pack(pady=20)
            self.eng_to_ukr_button.pack(side='left', padx=10)
            self.ukr_to_eng_button.pack(side='left', padx=10)
            self.back_button.pack_forget()  # Приховуємо кнопку Back
        else:
            self.category_frame.pack(pady=20)
            for widget in self.category_frame.winfo_children():
                widget.destroy()  # Очищаємо попередні кнопки категорій
            for category in self.categories:
                tk.Button(self.category_frame, text=category, command=lambda c=category: self.start_game(c)).pack(pady=5)
            self.back_button.pack(side='top', anchor='ne', padx=10, pady=10)  # Показуємо кнопку Back
        
        self.end_button.pack(side='top', anchor='ne', padx=10, pady=10)

    def set_direction(self, direction):
        print(f"Setting direction: {direction}")
        self.direction = direction
        self.update_dictionaries()
        self.direction_frame.pack_forget()
        self.show_categories()

