        for category in self.categories:
            if self.direction == "eng_to_ukr":
                self.categories[category] = {v: k for k, v in original_dictionaries[category].items()}
            else:  # ukr_to_eng
                self.categories[category] = original_dictionaries[category].copy()
        
    def start_game(self, category):
        print(f"Starting game with category: {category}")  # Додайте цей рядок для відлагодження
        self.start_time = datetime.now()  # Ініціалізувати start_time
        self.current_category = category
        self.category_label.config(text=f"Категорія: {category}")
        self.reset_ui()
        self.category_label.pack(pady=10)
        
        word_frame = tk.Frame(self.master)
        word_frame.pack(pady=10)
        
        self.word_label = tk.Label(word_frame, text="", font=("Arial", 24))
        self.word_label.pack(side='left', pady=5)
        
        self.sound_button = tk.Button(word_frame, text="🔊", command=self.play_word)
        self.sound_button.pack(side='left', padx=5)
        
        self.entry.pack(fill='x', padx=20, pady=10)
        self.check_button.pack(pady=10)
        self.result_label.pack(pady=10)
        self.button_frame.pack(pady=10)
        self.skip_button.pack(in_=self.button_frame, side='left', padx=5)
        self.end_button.pack(side='top', anchor='ne', padx=10, pady=10)
        self.back_button.pack(side='top', anchor='ne', padx=10, pady=10)
        
        self.next_word()
    
    def next_word(self):
        if not self.current_category:
            print("Error: No category selected")
            return  
        self.entry.delete(0, 'end')
        self.result_label.config(text="")
        self.next_button.pack_forget()
        self.retry_button.pack_forget()
        self.translate_button.pack_forget()

        if self.categories[self.current_category]:
            available_words = [word for word in self.categories[self.current_category].keys()
                               if self.stats[self.current_category]["word_count"].get(word, 0) < 5]
            if available_words:
                self.current_word = random.choice(available_words)
                print(f"Current word: {self.current_word}")  # Додайте цей рядок для відлагодження
                self.word_label.config(text=self.current_word)
                print(f"word_label text after update: {self.word_label.cget('text')}")  # Додайте цей рядок для відлагодження
                self.sound_button.pack(side='left', padx=5)
                self.stats[self.current_category]["generated"] += 1
                self.stats[self.current_category]["word_count"][self.current_word] = \
                    self.stats[self.current_category]["word_count"].get(self.current_word, 0) + 1
            else:
                print("No available words")  # Додайте цей рядок для відлагодження
                messagebox.showinfo("Кінець гри", "Всі слова в цій категорії були використані 5 разів!")
                self.show_categories()
        else:
            print("Empty category")  # Додайте цей рядок для відлагодження
            messagebox.showinfo("Кінець гри", "У цій категорії немає слів!")
            self.show_categories()
        
    def handle_enter(self, event):
        if self.result_label.cget("text") == "Правильно!":
            self.next_word()
        elif self.result_label.cget("text") == "Неправильно":
            if not self.translate_pressed:
                self.translate()
                self.translate_pressed = True
            elif self.translate_pressed:
                self.retry()
        else:
                self.check_answer()

    def check_answer(self):
        user_answer = self.entry.get().strip()
        correct_answer = self.categories[self.current_category][self.current_word]
        
        if user_answer.lower() == correct_answer.lower():
            self.result_label.config(text="Правильно!", fg="green")
            self.next_button.pack(side='left', padx=5)
            self.stats[self.current_category]["correct"] += 1
            self.stats[self.current_category]["correct_words"].append(self.current_word)
            self.translate_pressed = False  # Скидаємо стан
        else:
            self.result_label.config(text="Неправильно", fg="red")
            self.translate_button.pack(side='left', padx=5)
            self.stats[self.current_category]["incorrect"] += 1
            if self.current_word not in self.stats[self.current_category]["incorrect_words"]:
                self.stats[self.current_category]["incorrect_words"][self.current_word] = []
            self.stats[self.current_category]["incorrect_words"][self.current_word].append((user_answer, correct_answer))
            self.translate_pressed = False  # Скидаємо стан

    def retry(self):
        self.entry.delete(0, 'end')
        self.result_label.config(text="")
        self.retry_button.pack_forget()
        self.translate_button.pack_forget()
        self.stats[self.current_category]["retry"] += 1
        self.stats[self.current_category]["retried_words"].append(self.current_word)
        self.translate_pressed = False  # Скидаємо стан

    def translate(self):
        translation = self.categories[self.current_category][self.current_word]
        self.result_label.config(text=f"Переклад: {translation}", fg="blue")
        self.stats[self.current_category]["translated"] += 1
        self.stats[self.current_category]["translated_words"].append(self.current_word)
        self.translate_button.pack_forget()
        self.retry_button.pack(side='left', padx=5)

    def skip_word(self, event=None):
        self.stats[self.current_category]["skipped"] += 1
        self.stats[self.current_category]["skipped_words"].append(self.current_word)
        self.next_word()
        self.translate_pressed = False  # Скидаємо стан
        
    def pause_category_time(self):
        if self.current_category:
            end_time = datetime.now()
            start_time = self.category_start_times[self.current_category]
            if start_time:
                time_spent = (end_time - start_time).total_seconds()
                self.stats[self.current_category]["time_spent"] += time_spent
                self.category_start_times[self.current_category] = None
            
    def end_game(self):
        self.pause_category_time()  # Зупиняємо час для поточної категорії
        self.end_time = datetime.now()
        
        if self.current_category:
            end_time = datetime.now()
            
            if self.start_time is not None:
                time_spent = (end_time - self.start_time).total_seconds()
            else:
                time_spent = 0  # Handle case where start_time is None

            start_time = self.stats[self.current_category].get("start_time", self.start_time)
            time_spent = (end_time - start_time).total_seconds()
            self.stats[self.current_category]["time_spent"] += time_spent

        stats_text = f"Вітаю! Ви натиснули на кнопку 'Почати' {self.program_start_time.strftime('%d:%m:%Y, %H:%M:%S')}\n\n"
        stats_text += "Статистика:\n\n"

        stats_text += f"Напрямок перекладу: {'English to Ukrainian' if self.direction == 'eng_to_ukr' else 'Ukrainian to English'}\n\n"
        # Решта коду залишається без змін, але змініть цикл для статистики:
        for category, stat in self.stats.items():
            if stat["time_spent"] > 0:  # Показуємо тільки категорії, в яких був проведений час
                # Виводимо статистику для цієї категорії
                stats_text += f"Категорія {category}:\n"
                stats_text += f"Згенеровано слів: {stat['generated']}\n"
                stats_text += f"Правильно відповідей: {stat['correct']}\n"
                if stat['skipped'] > 0:
                    stats_text += f"Пропущено: {stat['skipped']}\n"
                if stat['translated'] > 0:
                    stats_text += f"Перекладено: {stat['translated']}\n"
                if stat['incorrect'] > 0:
                    stats_text += f"Неправильно відповідей: {stat['incorrect']}\n"
                if stat['retry'] > 0:
                    stats_text += f"Повторних спроб: {stat['retry']}\n"
                
                total_seconds = int(stat['time_spent'])
                hours, remainder = divmod(total_seconds, 3600)
                minutes, seconds = divmod(remainder, 60)
                stats_text += f"Ви провели {hours:02d}:{minutes:02d}:{seconds:02d}\n\n"

                stats_text += "Детальна статистика:\n"
                if stat['skipped_words']:
                    stats_text += f"Пропущені слова: {', '.join(stat['skipped_words'])}\n"
                if stat['incorrect_words']:
                    stats_text += "Неправильні відповіді:\n"
                    for word, wrong_answers in stat['incorrect_words'].items():
                        for wrong_answer, correct_answer in wrong_answers:
                            stats_text += f" {word}: {wrong_answer} (правильно: {correct_answer})\n"
                if stat['translated_words']:
                    stats_text += f"Перекладені слова: {', '.join(stat['translated_words'])}\n"
                if stat['retried_words']:
                    stats_text += f"Повторені слова: {', '.join(stat['retried_words'])}\n"
                if stat['correct_words']:
                    stats_text += f"Правильні відповіді: {', '.join(stat['correct_words'])}\n"
                stats_text += "\n"

        stats_window = tk.Toplevel(self.master)
        stats_window.title("Статистика")
        text_widget = tk.Text(stats_window, wrap=tk.WORD)
        text_widget.insert(tk.END, stats_text)
        text_widget.pack(expand=True, fill=tk.BOTH)

        button_frame = tk.Frame(stats_window)
        button_frame.pack(pady=10)
        ok_button = tk.Button(button_frame, text="OK", command=stats_window.destroy)
        ok_button.pack(side=tk.LEFT, padx=5)
        export_button = tk.Button(button_frame, text="Завантажити Exell", command=self.export_to_excel)
        export_button.pack(side=tk.LEFT, padx=5)

        stats_window.wait_window()

        response = messagebox.askyesno("Продовжити?", "Чи бажаєте Ви продовжити?")
        if response:
            self.game_started = False
            self.show_categories()
        else:
            self.master.destroy()
            sys.exit()

    def play_sound(self):
        # Ваш код для відтворення звуку
        pass 

    def export_to_excel(self):
        file_path = filedialog.asksaveasfilename(defaultextension=".xlsx", filetypes=[("Excel files", "*.xlsx")])
        if not file_path:
            return  # Користувач скасував вибір файлу

        wb = Workbook()
        ws = wb.active
        ws.title = "Статистика"

        # Стилі
        title_font = Font(name='Arial', size=14, bold=True, color="FFFFFF")
        header_font = Font(name='Arial', size=12, bold=True, color="FFFFFF")
    
        data_font = Font(name='Arial', size=11)
        
        title_fill = PatternFill(start_color="4472C4", end_color="4472C4", fill_type="solid")
        header_fill = PatternFill(start_color="5B9BD5", end_color="5B9BD5", fill_type="solid")
        total_fill = PatternFill(start_color="BDD7EE", end_color="BDD7EE", fill_type="solid")
        
        border = Border(left=Side(style='thin'), right=Side(style='thin'), top=Side(style='thin'), bottom=Side(style='thin'))
         # Додаємо нові стилі
        correct_fill = PatternFill(start_color="C6EFCE", end_color="C6EFCE", fill_type="solid")
        skipped_fill = PatternFill(start_color="FFEB9C", end_color="FFEB9C", fill_type="solid")
        # Заголовок
        ws.merge_cells('A1:I1')
        ws['A1'] = "Статистика тренування словникового запасу"
        ws['A1'].font = title_font
        ws['A1'].fill = title_fill
        ws['A1'].alignment = Alignment(horizontal="center", vertical="center")

        # Початкова інформація
        ws['A2'] = f"Початок сеансу: {self.program_start_time.strftime('%d:%m:%Y, %H:%M:%S')}"
        ws['A3'] = f"Кінець сеансу: {datetime.now().strftime('%d:%m:%Y, %H:%M:%S')}"
        directions = []
        if hasattr(self, 'eng_to_ukr_selected') and self.eng_to_ukr_selected:
            directions.append("English to Ukrainian")
        if hasattr(self, 'ukr_to_eng_selected') and self.ukr_to_eng_selected:
            directions.append("Ukrainian to English")

        row = 5
        for direction in directions:
            ws.cell(row=row, column=1, value=f"Напрямок перекладу: {direction}").font = header_font
            row += 1
        if len(directions) == 2:
            direction_text = "English to Ukrainian and Ukrainian to English"
        elif len(directions) == 1:
            direction_text = directions[0]
        else:
            direction_text = "Not specified"
        
        ws['A4'] = f"Напрямок перекладу: {direction_text}"
        # Заголовки таблиці
        headers = ["Категорія", "Час у категорії", "Згенеровано слів", "Правильно відповідей", 
                   "Пропущено", "Перекладено", "Неправильно відповідей", "Повторних спроб", "Оцінка"]
        for col, header in enumerate(headers, start=1):
            cell = ws.cell(row=5, column=col, value=header)
            cell.font = header_font
            cell.fill = header_fill
            cell.alignment = Alignment(horizontal="center", vertical="center")
            cell.border = border

        # Дані для кожної категорії
        row = 6
        for category, stat in self.stats.items():
            if stat["time_spent"] > 0:
                total_seconds = int(stat['time_spent'])
                hours, remainder = divmod(total_seconds, 3600)
                minutes, seconds = divmod(remainder, 60)
                time_spent = f"{hours:02d}:{minutes:02d}:{seconds:02d}"
                
                correct_percentage = (stat['correct'] / stat['generated']) * 100 if stat['generated'] > 0 else 0

                if 0 <= correct_percentage <= 20:
                    grade = "Даний рівень для тебе заважкий, потренуйся на попередньому - бал 1"
                elif 21 <= correct_percentage <= 40:
                    grade = "Даний рівень поки що не для тебе, потренуйся на попередньому - бал 2"
                elif 41 <= correct_percentage <= 60:
                    grade = "Ти потрохи починаєш розуміти даний рівень, та все ж потренуйся на попередньому - бал 3"
                elif 61 <= correct_percentage <= 95:
                    grade = "Даний рівень починає ставати твоїм, тренуйся в тому же дусі - бал 4"
                else:
                    grade = "Ти пройшов (-ла) даний рівень, можеш переходити до наступного або ж завершити і використовувати вже вивчені слова на практиці - бал 5"

                data = [category, time_spent, stat['generated'], stat['correct'], stat['skipped'],
                        stat['translated'], stat['incorrect'], stat['retry'], grade]
                
                for col, value in enumerate(data, start=1):
                    cell = ws.cell(row=row, column=col, value=value)
                    cell.font = data_font
                    cell.alignment = Alignment(horizontal="center", vertical="center")
                    cell.border = border
                
                row += 1

        # Додаємо рядок із загальним часом
        row += 1
        total_time = sum(stat['time_spent'] for stat in self.stats.values())
        hours, remainder = divmod(int(total_time), 3600)
        minutes, seconds = divmod(remainder, 60)
        total_time_spent = f"{hours:02d}:{minutes:02d}:{seconds:02d}"
        
        ws.merge_cells(f'A{row}:H{row}')
        total_cell = ws.cell(row=row, column=1, value="Загальний час:")
        total_cell.font = header_font
        total_cell.fill = total_fill
        total_cell.alignment = Alignment(horizontal="right", vertical="center")
        
        time_cell = ws.cell(row=row, column=9, value=total_time_spent)
        time_cell.font = header_font
        time_cell.fill = total_fill
        time_cell.alignment = Alignment(horizontal="center", vertical="center")
        
        for col in range(1, 10):
            ws.cell(row=row, column=col).border = border

        row += 2  # Пропуск рядка

            # Детальна статистика
               # Детальна статистика
        ws.cell(row=row, column=1, value="Детальна статистика").font = header_font
        row += 1

        for category, stat in self.stats.items():
            if stat["generated"] > 0:
                ws.cell(row=row, column=1, value=f"Категорія: {category}").font = header_font
                row += 1

                # Правильні відповіді
                if stat['correct_words']:
                    ws.cell(row=row, column=1, value="Правильні відповіді").font = data_font
                    ws.cell(row=row, column=1).fill = correct_fill
                    row += 1
                    for word in stat['correct_words']:
                        ws.cell(row=row, column=1, value=word)
                        ws.cell(row=row, column=1).fill = correct_fill
                        row += 1

                # Пропущені слова
                if stat['skipped_words']:
                    ws.cell(row=row, column=1, value="Пропущені слова").font = data_font
                    ws.cell(row=row, column=1).fill = skipped_fill
                    row += 1
                    for word in stat['skipped_words']:
                        ws.cell(row=row, column=1, value=word)
                        ws.cell(row=row, column=1).fill = skipped_fill
                        row += 1

                # Неправильні відповіді
                if stat['incorrect_words']:
                    ws.cell(row=row, column=1, value="Неправильні відповіді").font = data_font
                    row += 1
                    for word, wrong_answers in stat['incorrect_words'].items():
                        for wrong_answer, correct_answer in wrong_answers:
                            ws.cell(row=row, column=1, value=word)
                            ws.cell(row=row, column=2, value=wrong_answer)
                            ws.cell(row=row, column=3, value=correct_answer)
                            row += 1

                # Перекладені слова
                if stat['translated_words']:
                    ws.cell(row=row, column=1, value="Перекладені слова").font = data_font
                    row += 1
                    ws.cell(row=row, column=1, value=", ".join(stat['translated_words']))
                    row += 1

                row += 1

        # Висновок
        ws.cell(row=row, column=1, value="Висновок").font = header_font
        row += 1

        for category, stat in self.stats.items():
            if stat["generated"] > 0:
                correct_percentage = (stat['correct'] / stat['generated']) * 100
                translated_percentage = (stat['translated'] / stat['generated']) * 100
                skipped_percentage = (stat['skipped'] / stat['generated']) * 100
                score = 1 + min(4, int(correct_percentage // 20))

                ws.cell(row=row, column=1, value=f"Категорія {category}").font = header_font
                row += 1
                ws.cell(row=row, column=1, value="Показник")
                ws.cell(row=row, column=2, value="Значення")
                row += 1
                ws.cell(row=row, column=1, value="Бал")
                ws.cell(row=row, column=2, value=score)
                row += 1
                ws.cell(row=row, column=1, value="Відсоток правильних відповідей")
                ws.cell(row=row, column=2, value=f"{correct_percentage:.2f}%")
                row += 1
                ws.cell(row=row, column=1, value="Відсоток перекладених слів")
                ws.cell(row=row, column=2, value=f"{translated_percentage:.2f}%")
                row += 1
                ws.cell(row=row, column=1, value="Відсоток пропущених слів")
                ws.cell(row=row, column=2, value=f"{skipped_percentage:.2f}%")
                row += 2

        # Автоматичне регулювання ширини стовпців
        for column_cells in ws.columns:
            length = max(len(str(cell.value)) for cell in column_cells)
            ws.column_dimensions[get_column_letter(column_cells[0].column)].width = length + 2

        wb.save(file_path)
        messagebox.showinfo("Експорт завершено", f"Статистику збережено у файл {file_path}")

    def play_word(self):
        if self.current_word:
            print(f"Playing word: {self.current_word}")  # Додайте цей рядок для відлагодження
            word = self.current_word
            lang = 'en' if self.direction == 'eng_to_ukr' else 'uk'
            tts = gTTS(text=word, lang=lang)
            tts.save("temp.mp3")
            playsound("temp.mp3")
            os.remove("temp.mp3")
        else:
            print("No current word to play")  # Додайте цей рядок для відлагодження
    
    def reset_ui(self):
        for widget in self.master.winfo_children():
            widget.pack_forget()

        self.end_button.pack(side='top', anchor='ne', padx=10, pady=10)
        self.back_button.pack(side='top', anchor='ne', padx=10, pady=10)

        if self.current_category:
            self.category_label.pack(pady=10)
            
            word_frame = tk.Frame(self.master)
            word_frame.pack(pady=10)
            
            self.word_label = tk.Label(word_frame, text="", font=("Arial", 24))
            self.word_label.pack(side='left', pady=5)
            
            self.sound_button = tk.Button(word_frame, text="🔊", command=self.play_word)
            self.sound_button.pack(side='left', padx=5)
            
            self.entry.pack(fill='x', padx=20, pady=10)
            self.check_button.pack(pady=10)
            self.result_label.pack(pady=10)
            self.button_frame.pack(pady=10)
            self.skip_button.pack(in_=self.button_frame, side='left', padx=5)
        elif self.direction:
            self.category_frame.pack(pady=20)
        else:
            self.direction_frame.pack(pady=20)
            self.eng_to_ukr_button.pack(side='left', padx=10)
            self.ukr_to_eng_button.pack(side='left', padx=10)

        self.result_label.config(text="")
        self.entry.delete(0, 'end')
    
def run_program():
    root = tk.Tk()
    trainer = VocabularyTrainer(root)
    root.mainloop()

if __name__ == "__main__":
    run_program()
         
